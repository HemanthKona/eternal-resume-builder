{
  "version": 1,
  "projects": {
    "resume-builder": {
      "root": "apps/resume-builder",
      "sourceRoot": "apps/resume-builder/src",
      "projectType": "application",
      "schematics": {},
      "architect": {
        "build": {
          "builder": "@nrwl/web:build",
          "options": {
            "outputPath": "apps/resume-builder/publish",
            "index": "apps/resume-builder/src/index.html",
            "main": "apps/resume-builder/src/main.tsx",
            "polyfills": "apps/resume-builder/src/polyfills.ts",
            "tsConfig": "apps/resume-builder/tsconfig.app.json",
            "assets": [
              "apps/resume-builder/src/favicon.ico",
              "apps/resume-builder/src/assets",
              "apps/resume-builder/src/_redirects"
            ],
            "styles": [],
            "scripts": [],
            "webpackConfig": "@nrwl/react/plugins/webpack"
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/resume-builder/src/environments/environment.ts",
                  "with": "apps/resume-builder/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                }
              ]
            }
          },
          "outputs": [
            "{options.outputPath}"
          ]
        },
        "serve": {
          "builder": "@nrwl/web:dev-server",
          "options": {
            "buildTarget": "resume-builder:build",
            "proxyConfig": "apps/resume-builder/proxy.conf.json"
          },
          "configurations": {
            "production": {
              "buildTarget": "resume-builder:build:production"
            }
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "apps/resume-builder/**/*.js",
              "apps/resume-builder/**/*.jsx",
              "apps/resume-builder/**/*.ts",
              "apps/resume-builder/**/*.tsx",
              "apps/resume-builder/**/*.spec.ts",
              "apps/resume-builder/**/*.spec.tsx",
              "apps/resume-builder/**/*.spec.js",
              "apps/resume-builder/**/*.spec.jsx",
              "apps/resume-builder/**/*.d.ts"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "apps/resume-builder/jest.config.js",
            "passWithNoTests": true
          },
          "outputs": [
            "coverage/apps/resume-builder"
          ]
        }
      }
    },
    "resume-builder-e2e": {
      "root": "apps/resume-builder-e2e",
      "sourceRoot": "apps/resume-builder-e2e/src",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/resume-builder-e2e/cypress.json",
            "tsConfig": "apps/resume-builder-e2e/tsconfig.e2e.json",
            "devServerTarget": "resume-builder:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "resume-builder:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "apps/resume-builder-e2e/src/**/*.ts",
              "apps/resume-builder-e2e/src/**/*.js"
            ]
          }
        }
      }
    },
    "api": {
      "root": "apps/api",
      "sourceRoot": "apps/api/src",
      "projectType": "application",
      "prefix": "api",
      "schematics": {},
      "architect": {
        "build": {
          "builder": "@nrwl/node:build",
          "options": {
            "outputPath": "dist/apps/api",
            "main": "apps/api/src/main.ts",
            "tsConfig": "apps/api/tsconfig.app.json",
            "assets": [
              "apps/api/src/assets"
            ]
          },
          "configurations": {
            "production": {
              "optimization": true,
              "extractLicenses": true,
              "inspect": false,
              "fileReplacements": [
                {
                  "replace": "apps/api/src/environments/environment.ts",
                  "with": "apps/api/src/environments/environment.prod.ts"
                }
              ]
            }
          },
          "outputs": [
            "{options.outputPath}"
          ]
        },
        "serve": {
          "builder": "@nrwl/node:execute",
          "options": {
            "buildTarget": "api:build"
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "apps/api/**/*.ts",
              "apps/api/**/*.spec.ts",
              "apps/api/**/*.d.ts"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "apps/api/jest.config.js",
            "passWithNoTests": true
          },
          "outputs": [
            "coverage/apps/api"
          ]
        }
      }
    },
    "api-interfaces": {
      "root": "libs/api-interfaces",
      "sourceRoot": "libs/api-interfaces/src",
      "projectType": "library",
      "schematics": {},
      "architect": {
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/api-interfaces/**/*.ts",
              "libs/api-interfaces/**/*.spec.ts",
              "libs/api-interfaces/**/*.spec.tsx",
              "libs/api-interfaces/**/*.spec.js",
              "libs/api-interfaces/**/*.spec.jsx",
              "libs/api-interfaces/**/*.d.ts"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "libs/api-interfaces/jest.config.js",
            "passWithNoTests": true
          },
          "outputs": [
            "coverage/libs/api-interfaces"
          ]
        }
      }
    },
    "data": {
      "root": "libs/data",
      "sourceRoot": "libs/data/src",
      "projectType": "library",
      "schematics": {},
      "architect": {
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/data/**/*.ts",
              "libs/data/**/*.spec.ts",
              "libs/data/**/*.spec.tsx",
              "libs/data/**/*.spec.js",
              "libs/data/**/*.spec.jsx",
              "libs/data/**/*.d.ts"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "libs/data/jest.config.js",
            "passWithNoTests": true
          },
          "outputs": [
            "coverage/libs/data"
          ]
        }
      }
    },
    "util": {
      "root": "libs/util",
      "sourceRoot": "libs/util/src",
      "projectType": "library",
      "schematics": {},
      "architect": {
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/util/**/*.js",
              "libs/util/**/*.jsx",
              "libs/util/**/*.ts",
              "libs/util/**/*.tsx",
              "libs/util/**/*.spec.ts",
              "libs/util/**/*.spec.tsx",
              "libs/util/**/*.spec.js",
              "libs/util/**/*.spec.jsx",
              "libs/util/**/*.d.ts"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "libs/util/jest.config.js",
            "passWithNoTests": true
          },
          "outputs": [
            "coverage/libs/util"
          ]
        }
      }
    },
    "themes": {
      "root": "libs/themes",
      "sourceRoot": "libs/themes/src",
      "projectType": "library",
      "schematics": {},
      "architect": {
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/themes/**/*.js",
              "libs/themes/**/*.jsx",
              "libs/themes/**/*.ts",
              "libs/themes/**/*.tsx",
              "libs/themes/**/*.spec.ts",
              "libs/themes/**/*.spec.tsx",
              "libs/themes/**/*.spec.js",
              "libs/themes/**/*.spec.jsx",
              "libs/themes/**/*.d.ts"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "libs/themes/jest.config.js",
            "passWithNoTests": true
          },
          "outputs": [
            "coverage/libs/themes"
          ]
        }
      }
    }
  },
  "cli": {
    "defaultCollection": "@nrwl/react"
  },
  "schematics": {
    "@nrwl/workspace": {
      "library": {
        "linter": "eslint"
      }
    },
    "@nrwl/cypress": {
      "cypress-project": {
        "linter": "eslint"
      }
    },
    "@nrwl/react": {
      "application": {
        "style": "",
        "linter": "eslint",
        "babel": true
      },
      "component": {
        "style": ""
      },
      "library": {
        "style": "",
        "linter": "eslint"
      }
    },
    "@nrwl/next": {
      "application": {
        "linter": "eslint"
      }
    },
    "@nrwl/web": {
      "application": {
        "linter": "eslint"
      }
    },
    "@nrwl/node": {
      "application": {
        "linter": "eslint"
      },
      "library": {
        "linter": "eslint"
      }
    },
    "@nrwl/nx-plugin": {
      "plugin": {
        "linter": "eslint"
      }
    },
    "@nrwl/nest": {
      "application": {
        "linter": "eslint"
      }
    },
    "@nrwl/express": {
      "application": {
        "linter": "eslint"
      },
      "library": {
        "linter": "eslint"
      }
    }
  },
  "defaultProject": "resume-builder"
}